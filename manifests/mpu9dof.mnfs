;
; Click Board Manifest Source for
; MPU 9DOF Click
; https://www.mikroe.com/mpu-9dof-click
;

# -----------------------------------------------------------
#            Manifest Header
# -----------------------------------------------------------
[manifest-header]
version-major = 0
version-minor = 1
click-string-id = 1
num-devices = 2
int-gpio-state = 1

# -----------------------------------------------------------
#            Manifest String Descriptors
# -----------------------------------------------------------
; click string (id can't be 0)
[string-descriptor 1]
string = mpu9dof

; driver string
[string-descriptor 2]
string = mpu9150

; driver string
[string-descriptor 3]
string = ak8975

# -----------------------------------------------------------
#            Manifest Device Descriptors
# -----------------------------------------------------------
; device descriptor
[device-descriptor 1]
driver-string-id = 2
protocol = 2
reg = 0x68
irq = 1
irq-type = 1

; device descriptor
[device-descriptor 2]
driver-string-id = 3
protocol = 5
reg = 0x0c